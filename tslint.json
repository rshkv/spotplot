{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-react"
    ],
    "rules": {
        "quotemark": [true, "single", "jsx-double"],
        "ordered-imports": [
            true,
            {
                "import-sources-order": "case-insensitive",
                "named-imports-order": "any"
            }
        ],
        "arrow-parens": false,
        "jsx-no-multiline-js": false,
        "curly": false,
        "object-literal-sort-keys": false,
        "no-unused-variable": true
    }
}